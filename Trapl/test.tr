
Container<gen T>
{
	data: gen T
}


ContainerOfContainer<gen T<gen U>>
{
	data: gen T<gen U>
}


put_in_container<gen T> (container: &Container<gen T>, value: gen T)
	{ (@container).data = value }


ContainerCollection
{
	a: Container<Int32>,
	b: Container<Container<Int32>>,
	c: ContainerOfContainer<Container<Int32>>
}


test()
{
	let tests: ContainerCollection;
	put_in_container:<Int32>(&tests.a, 42);
}